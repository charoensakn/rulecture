{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099,
      "host": "192.168.0.12"
    },
    "firestore": {
      "port": 8080,
      "host": "192.168.0.12"
    },
    "database": {
      "port": 9000,
      "host": "192.168.0.12"
    },
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    },
    "storage": {
      "port": 9199,
      "host": "192.168.0.12"
    },
    "ui": {
      "enabled": true
    }
  }
}
